<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
	<PackageId>Octopus</PackageId>
	<Title>Octopus</Title>
	<Version>1.0.0</Version>
	<Authors>Luis Domingo Rueda Wilches</Authors>
	<Company>Luis Domingo Rueda Wilches</Company>
	<Product>Octopus</Product>
	<Description>Octopus is a Dot Net 7 Library, Methods of extensions, resources.</Description>
	<Copyright>Copyright Â© Luis Domingo Rueda Wilches 2022</Copyright>
	<RepositoryUrl>https://github.com/ingluisruedaw/Octopus</RepositoryUrl>
	<RepositoryType>git</RepositoryType>
	<PackageReadmeFile>README.md</PackageReadmeFile>
	<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
	<AnalysisLevel>latest</AnalysisLevel>
	<EnableNETAnalyzers>True</EnableNETAnalyzers>
	<SignAssembly>False</SignAssembly>
  </PropertyGroup>

  <ItemGroup>
	<None Include="..\Octopus\README.md">
	  <Pack>True</Pack>
	  <PackagePath>\</PackagePath>
	</None>
  </ItemGroup>

</Project>
